<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="indi.Lucius.mapper.UserMapper">

    <select id="selectUserByAccAndPwd" resultType="indi.Lucius.pojo.UserPojo">
        Select * from user_info where user_Acc = #{userAcc} and user_pwd = #{userPwd} and d_state = 1;
    </select>
</mapper>